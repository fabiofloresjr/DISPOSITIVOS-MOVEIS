<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListagemJogosActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:onClick="abreTelaAtualizacao"
            android:text="Atualizar Jogos" />

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:onClick="abreTelaDelete"
            android:text="Deletar Jogos" />

        <Button
            android:id="@+id/buttonVoltaCadastro"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:onClick="abreTelaCadastro"
            android:text="Voltar Ã  tela de cadastro" />

        <TextView
            android:id="@+id/textViewListagemJogos"
            android:layout_width="match_parent"
            android:layout_height="659dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>